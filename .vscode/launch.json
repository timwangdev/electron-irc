  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main Process",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "env": {
        "ELECTRON_START_URL": "http://localhost:3000",
        "NODE_DEBUG": "net,irc"
      },
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "program": "${workspaceRoot}/src/main/index.js"
    }
  ]
}